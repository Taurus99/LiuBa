<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.navinfo.liuba">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.navinfo.liuba.RegisterActivity"
            android:label="注册">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.navinfo.liuba.BaseActivity"></activity>
        <activity android:name="com.navinfo.liuba.MainActivity"></activity>
        <activity
            android:name="com.navinfo.liuba.OrderInputActivity"
            android:label="订单填写"></activity>
        <activity
            android:name="com.navinfo.liuba.OrderConfirmActivity"
            android:label="订单确认"></activity>
        <activity
            android:name="com.navinfo.liuba.MyOrderListActivity"
            android:label="订单列表"></activity>
        <activity
            android:name="com.navinfo.liuba.PaymentSelect"
            android:label="支付"></activity>
        <activity
            android:name="com.navinfo.liuba.PaymentConfirm"
            android:label="支付成功"></activity>
        <activity
            android:name="com.navinfo.liuba.MyScretActivity"
            android:label="密码锁"></activity>
    </application>

</manifest>
